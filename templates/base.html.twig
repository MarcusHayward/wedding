<!DOCTYPE html>
<html lang="en">
<head>
    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('/apple-touch-icon.png') }}">
        <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('/favicon-32x32.png') }}">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('/favicon-16x16.png') }}">
        <link rel="manifest" href="{{ asset('manifest.json') }}/">
        <link rel="mask-icon" href="{{ asset('/safari-pinned-tab.svg') }}" color="#5bbad5">
        <meta name="apple-mobile-web-app-title" content="Wedding">
        <meta name="application-name" content="Wedding">
        <meta name="theme-color" content="#ffffff">

        <title>{{ block('title') }}</title>

        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600" rel="stylesheet">
            <link rel="stylesheet" href="{{ asset('build/global.css') }}">
        {% endblock stylesheets %}
    {% endblock head %}
</head>

<body>
{% block header %}
{% endblock %}
{% block body %}
    {{ block('content') }}
{% endblock body %}
{% block footer %}
    {% include 'blocks/footer.html.twig' %}
{% endblock %}

<script src="{{ asset('build/app.js') }}" language="javascript" type="text/javascript"></script>

</body>
</html>
